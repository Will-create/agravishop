// Common
homepage          : @(Acceuil)            --> /
posts             : @(Blog)                --> @(/blog/)                 --> homepage
post              : @(Post)                --> @(/blog/{0}/)             --> posts
notices           : @(Avis)             --> @(/avis/)              --> homepage

// Admin area
admin             : @(Admin)               --> @(/admin/*)

// Eshop
checkout          : @(Soumission de la commande)            --> @(/soumission-de-la-commande/)            --> homepage
category          : @(Tous les produits)        --> @(/categories/*)         --> homepage
detail            : @(Details produit)      --> @(/detail/{0}/)          --> category
top               : @(Produits TOP)        --> @(/top/)                 --> homepage
new               : @(Nouveau produits)        --> @(/nouveau/)                 --> homepage
popular           : @(Produits populaires)    --> @(/populaire/)             --> homepage
account           : @(Mon Compte)          --> @(/Compte/)             --> homepage
order             : @(Details commandes)        --> @(/soumission-de-la-commande/{0}/)        --> account
ligdicash             : @(Status depaiment)        --> @(/order/ligdicash/check-status/{0}/)        --> account
settings          : @(Parametres de compte)    --> @(/compte/parametres/)    --> account
logoff            : @(Deconnexion)              --> @(/deconnexion/)              --> homepage
